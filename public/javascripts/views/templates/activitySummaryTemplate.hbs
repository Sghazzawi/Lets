<div class="col-md-6 activity">
  <div class="innerActivity">
      <button type="button" class="close" aria-hidden="true">&times;</button>
      <h3>{{title}}</h3>
    {{#each tags}}
    <h6><span class="label label-info">{{this}}</span></h6>
    {{/each}}
    <hr>
    {{#each links}}
    <div class="row">
      <div class="col-md-8">
        <a href="#" class="thumbnail">
          <img src="/letsdoit/images/webThumbs/{{thumbnail}}" alt="{{url}}">
        </a>
      </div>
    </div>
    {{/each}}
    <div class="activityOptions" align="center">
      <button type="button" class="btn btn-default note">
        <span class="glyphicon glyphicon-pencil "></span>
      </button>
      <button type="button" class="btn btn-default url">
        <span class="glyphicon glyphicon-link "></span>
      </button>
      <button type="button" class="btn btn-default tag">
        <span class="glyphicon glyphicon-tag "></span>
      </button>
      <button type="button" class="btn btn-default schedule">
        <span class="glyphicon glyphicon-calendar "></span>
      </button>
    </div>
  </div>
</div>
