<div class="activityHeader col-md-12">
  <button type="button" class="close" aria-hidden="true"><span class="glyphicon glyphicon-trash"></span></button>
  <h1>{{title}}</h1>
  <div class="row">
    {{#each tags}}
      <div class="col-md-1">
        <h6><span class="label label-info">{{this}}</span></h6>
      </div>
    {{/each}}
  </div>
  <hr>
</div>
<div class="activityContent col-md-12">
  {{#each links}}
      <a href="{{url}}">
      <div class="col-md-3" >
        <div class="thumbnail">
          <img src="/letsdoit/images/webThumbs/{{thumbnail}}" alt="{{url}}">
        </div>
      </div>
      </a>
  {{/each}}
</div>
